start
    // Arithmetic operations
    number a = 10;
    number b = 5;
    decimal c = 2.5;
    
    number sum = a + b;          // 15
    number diff = a - b;         // 5
    decimal product = a * c;     // 25.0
    number quotient = a / b;     // 2
    number remainder = a % b;    // 0
    number power = a ** b;       // 100000

end