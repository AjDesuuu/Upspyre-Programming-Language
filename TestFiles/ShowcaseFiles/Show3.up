start
  // Testing repeat-until
  number count = 1;

  repeat{
    show(count);
    count = count + 1;
  } until(count > 5);

  // Testing for loop
  for (number i = 1; i <= 5; i = i + 1) {
    show("n: " + i.toText());
  }

  // Testing stop and continue
  number counter = 0;
  repeat (counter < 10) {
    counter = counter + 1;
    if (counter == 5) {
      continue;  // Skip printing 5
    }
    show(counter);
    if (counter == 8) {
      stop;  // Stops loop when counter reaches 8
    }
  }
end